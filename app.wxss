/**app.wxss**/
.container {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
}

.page{
  align-items: center;
}

.normal-button{
  color: #ffffff;
  background-color: #e64340;
  width:120px;
  margin-top: 10px;
  margin-bottom: 5px;
  font-size: 15px;
}
.title{
  text-align: center;
  width: fit-content;
  margin: auto;
  font-family: Simsun;
  font-size: 30px;
  color: black;
  margin-bottom: 60rpx;
  align-items: center;
  border-bottom: 3px solid #eeeeee;
  border-width: 1px;
}


.my-picker{
  height: 45px;
  display: flex;
  flex-direction: row;
  justify-items: start;
  align-items: center;
  border-bottom: 3px solid #eeeeee;
  border-width: 1px;
}

.my-picker-icon{
  height: 18px;
  width: 18px;
  margin-left: 14px;
}

.my-picker-label{
  font-size: 14px;
  color: #616161;
  margin-left: 3px;
}

.my-picker-input{
  margin-left: 42px;
}


.choose-image{
  height: 300rpx;
  width: 94%;
  background-size: contain;
  margin-top: 10px;
  margin-left: 2%;
  border: 3px solid #eeeeee;
}


.btncheck{
  margin: auto;

  }
.btn1check{
  display: flex;
  justify-content: center;
  margin-top: 20px;
}
